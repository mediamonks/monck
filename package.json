{
  "name": "@mediamonks/monck",
  "version": "1.0.0-alpha.6",
  "description": "Add highly configurable API mocks to your express server",
  "keywords": ["mock", "express", "middleware"],
  "repository": "git@github.com:mediamonks/monck.git",
  "author": "Arjan van Wijk <thanarie@gmail.com>",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "example": "nodemon --ext ts --exec 'ts-node' example/index.ts",
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc -p tsconfig.build.cjs.json",
    "build:esm": "tsc -p tsconfig.build.esm.json"
  },
  "dependencies": {
    "@types/express": "4.17.11",
    "@types/body-parser": "1.19.0",
    "@types/express-serve-static-core": "4.17.19",
    "@types/qs": "6.9.6",
    "@types/serve-static": "1.13.9",
    "@types/glob": "7.1.3",
    "@types/multer": "1.4.5",
    "@types/node": "14.14.13",
    "@types/path-to-regexp": "1.7.0",
    "@types/signale": "1.4.1",
    "asset": "^0.4.13",
    "body-parser": "^1.19.0",
    "chokidar": "^3.4.3",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "multer": "^1.4.2",
    "path-to-regexp": "^6.2.0",
    "signale": "^1.4.0"
  },
  "devDependencies": {
    "@types/faker": "^5.1.5",
    "faker": "^5.1.0",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
